

Host *.amazonaws.com
	User ubuntu
	IdentityFile ~/.ssh/aws.key
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	LogLevel QUIET
	ForwardAgent yes

Host *.aws.yogendra.me
	User ubuntu
	IdentityFile ~/.ssh/aws.key
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null
	LogLevel QUIET
	ForwardAgent yes
